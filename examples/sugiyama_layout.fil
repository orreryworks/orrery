diagram component [layout_engine="sugiyama"];

// Define some custom types
type Service = Rectangle [fill_color="#cdb4db"];
type Database = Rectangle [fill_color="#a2d2ff"];

// Create nodes with simple names (avoid complex shapes for now)
a as "ServiceA": Service;
b as "ServiceB": Service;
c as "ServiceC": Service;
d as "DatabaseD": Database;
e as "DatabaseE": Database;

// Define relationships - create a simple acyclic graph for Sugiyama layout
a -> b;
a -> c;
b -> d;
c -> d;
c -> e;